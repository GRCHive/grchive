storage "postgresql" {
    connection_url = "postgres://${POSTGRES_USER}:${POSTGRES_PASSWORD}@${POSTGRES_HOST}:${POSTGRES_PORT}/vault?sslmode=disable&timezone=UTC"
    table = "vault_kv_store"
}

listener "tcp" {
    address     = "localhost:${VAULT_PORT}"
    tls_disable = 1
}

disable_mlock = true
