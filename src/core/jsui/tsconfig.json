{
	"compilerOptions": {
		"target": "es5",
		"strict": true,
		"module": "esnext",
		"moduleResolution": "node",
        "lib": [
            "dom",
            "es5",
            "es2018",
            "es2018.promise"
        ],
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true
	},
    "include": [
        "./**/*.ts",
        "./**/*.vue"
    ],
    "exclude": [
        "node_modules"
    ]
}
